# Admin Client Environment Variables Example
# Copy this file to .env for local development
# For production, create a .env.production file with your production values

# API URLs
# For local development
VITE_API_URL=http://localhost:3000/api
VITE_BOT_SERVICE_URL=http://localhost:3100

# For production (replace with your actual API endpoints)
# VITE_API_URL=https://api.gringo.ezyy.cloud/api
# VITE_BOT_SERVICE_URL=https://bots.gringo.ezyy.cloud

# Bot API Key (for direct communication with bot microservice)
VITE_BOT_API_KEY=dev-bot-api-key

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true


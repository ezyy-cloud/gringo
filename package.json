{
  "name": "gringo",
  "version": "1.0.0",
  "description": "Real-time social networking application with Express, React, and Socket.IO",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "socket.io",
    "express",
    "react",
    "real-time",
    "social",
    "networking"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "cloudinary": "^2.5.1",
    "cloudinary-react": "^1.8.1",
    "react-webcam": "^7.2.0"
  }
}
